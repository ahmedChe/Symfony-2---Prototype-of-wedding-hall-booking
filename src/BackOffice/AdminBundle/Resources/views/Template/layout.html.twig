            <div class="media-body">
                <h5 class="media-heading"> Admin</h5>
                <ul class="list-unstyled user-info">

                    <li>
                        <a class="btn btn-success btn-xs btn-circle" style="width: 10px;height: 12px;"></a> Online

                    </li>

                </ul>
            </div>
            <br />
        </div>

        <ul id="menu" class="collapse">


            <li class="panel active">
                <a href="{{ path('back_office_admin_homepage') }}" >
                    <i class="icon-table"></i> Accueil


                </a>
            </li>
            <li><a href="{{ path('back_office_admin_resoonsables') }}"><i class="fa fa-users"></i> Responsables </a></li>
            <li><a href="{{ path('back_office_admin_clients') }}"><i class="fa fa-users"></i> Clients </a></li>


