                            {% block panelHead %}
                            {% endblock %}
                        </div>


                        <div class="demo-container">
                            {% block panelContent %}
                            {% endblock %}
                        </div>

                    </div>

                </div>



            </div>
            <!--END CHAT & CHAT SECTION -->
            <!-- COMMENT AND NOTIFICATION  SECTION -->




