        <script>
            function getLink()
            {
                var path=location.pathname.split("/")[4];
                var add="%20";
                if (path.indexOf(add)>-1)
                {
                    path.replace(add,"")
                }
                var link=document.getElementById("mainNav").getElementsByTagName("li");
                for(var i= 0; i < link.length; i++)
                {
                    if (link[i].getElementsByClassName('scroll-link')[0].innerHTML==path)
                    {
                        link[i].className="active";
                    }
                    else
                    {
                        link[i].className="";
                    }
                }

            }
        </script>
    {% endblock  %}
